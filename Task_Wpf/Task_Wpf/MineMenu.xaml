<Window x:Class="Task_Wpf.MineMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Task_Wpf"
        mc:Ignorable="d"
        Title="MineMenu" Height="650" Width="800">
    <Border BorderThickness="5" BorderBrush="Aqua">
    <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"></RowDefinition>
                <RowDefinition Height="12*"></RowDefinition>
                <RowDefinition Height="*"></RowDefinition>
            </Grid.RowDefinitions>

            <StackPanel Grid.Row="0" Background="Gray" Height="auto" Width="auto"></StackPanel>
                <Frame Grid.Row="1" x:Name="frame" Height="auto" Width="auto" LoadCompleted="frame_LoadCompleted">

                </Frame>

            
        
            <StackPanel Grid.Row="2" Background="Gray" Height="auto" Width="auto">

            </StackPanel>

        </Grid>
    </Border>
</Window>
